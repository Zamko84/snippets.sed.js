res.data.status.toLowerCase() !== 'active'
) {
Ext.Msg.show({
    title: gettext('No valid subscription'),
    icon: Ext.Msg.WARNING,
    message: Proxmox.Utils.getNoSubKeyHtml(res.data.url),
    buttons: Ext.Msg.OK,
    callback: function (btn) {
        if (btn !== 'ok') {
            return;
        }
        orig_cmd();
    },
});
}